<?xml version="1.0"?>
<robot name="barracuda">

	<material name="grey">
		<color rgba="0.75 0.75 0.75 1"/>
	</material>
	<material name="blue">
		<color rgba="0 0 0.8 1"/>
	</material>
	<material name="red">
		<color rgba="0.8 0 0 1"/>
	</material>
	<material name="green">
		<color rgba="0 0.8 0 1"/>
	</material>

	<gazebo>
		<plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
			<robotNamespace>/barracuda</robotNamespace>
		</plugin>
	</gazebo>


	<link name="base_link">
    		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
        		<mass value="3.2" />
        		<inertia ixx="4.46666666667e-02" ixy="0.0" ixz="0.0" iyy="4.46666666667e-02" iyz="0.0" izz="4e-04"/>
    		</inertial>
    		<collision>
        		<origin xyz="0 -0.2 0" rpy="1.6 0 0"/>
			<geometry>
            			<cylinder radius="0.05" length="0.4"/>
        		</geometry>
    		</collision>
        	<visual>
        		<origin rpy="1.6 0 0" xyz="0 -0.2 0"/>
        		<geometry>
            			<cylinder radius="0.05" length="0.4"/>
        		</geometry>
			<material name="grey"/>
        	</visual>
	</link>

	<gazebo reference="base_link">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
		<mu2>10.0</mu2>
		<material>Gazebo/Grey</material>
	</gazebo>

	<link name="second_link">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
        		<mass value="0.85" />
        		<inertia ixx="6.56625e-03" ixy="0.0" ixz="0.0" iyy="6.56625e-03" iyz="0.0" izz="3.825e-04"/>
    		</inertial>
    		<collision>
        		<origin xyz="0 0 0" rpy="1.6 0 0"/>
        		<geometry>
				<cylinder radius="0.03" length="0.3"/>
        		</geometry>
    		</collision>
        	<visual>
        		<origin rpy="1.6 0 0" xyz="0 0 0"/>
        		<geometry>
            			<cylinder radius="0.03" length="0.3"/>
        		</geometry>
			<material name="grey"/>
        	</visual>
	</link>  

	<gazebo reference="second_link">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
 		<mu2>10.0</mu2>
		<material>Gazebo/Grey</material>
	</gazebo>

	<joint name="middle_joint" type="fixed">
		<parent link="base_link"/>
		<child link="second_link"/>
		<origin xyz="0 -0.55 -0.01" rpy="0 0 0"/>
	</joint>

	<link name="ball_link">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.1" />
			<inertia ixx="3.6e-05" ixy="0.0" ixz="0.0" iyy="3.6e-05" iyz="0.0" izz="3.6e-05"/>
		</inertial>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<sphere radius="0.03"/>
			</geometry>
		</collision>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<sphere radius="0.03"/>
			</geometry>
			<material name="green"/>
		</visual>
	</link>

	<gazebo reference="ball_link">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
		<mu2>10.0</mu2>
		<material>Gazebo/Green</material>
	</gazebo>
    
	<joint name="ball_joint" type="fixed">
		<parent link="second_link"/>
		<child link="ball_link"/>
		<origin xyz="0 -0.15 -0.004" rpy="0 0 0"/>
	</joint>

	<link name="midball_link">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.5" />
			<inertia ixx="5e-04" ixy="0.0" ixz="0.0" iyy="5e-04" iyz="0.0" izz="5e-04"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
        		<geometry>
                		<sphere radius="0.05"/>
        		</geometry>
			<material name="grey"/>
        	</visual>
	</link>

	<gazebo reference="midball_link">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
		<mu2>10.0</mu2>
		<material>Gazebo/Grey</material>
	</gazebo>
    
	<joint name="midball_joint" type="fixed">
		<parent link="base_link"/>
		<child link="midball_link"/>
		<origin xyz="0 -0.4 -0.005" rpy="0 0 0"/>
	</joint>  

	<link name="fin_vup"> fins
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.005" />
			<inertia ixx="2.08333333333e-06" ixy="0.0" ixz="0.0" iyy="1.04333333333e-06" iyz="0.0" izz="1.04333333333e-06"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 -0.05 0.05"/>
			<geometry>
				<box size="0.002 0.05 0.05"/> x-length,y-legth and z-length
			</geometry>
			<material name="red"/>
		</visual>
	</link>

	<gazebo reference="fin_vup">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
		<mu2>10.0</mu2>
		<material>Gazebo/Red</material>
	</gazebo>

	<joint name="fin_vup_joint" type="revolute">
		<parent link="base_link"/>
		<child link="fin_vup"/>
		<origin xyz="0 0 0.05" rpy="0 0 0"/>
		<limit lower="-0.2" upper="0.2" effort="0.1" velocity="0.005"/>
		<axis xyz="0 0 1"/>
	</joint>

	<transmission name="tran1">
		<type>transmission_interface/SimpleTransmission</type>
		<joint name="fin_vup_joint">
			<hardwareInterface>EffortJointInterface</hardwareInterface>
		</joint>
		<actuator name="motor1">
			<hardwareInterface>EffortJointInterface</hardwareInterface>
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>

	<link name="fin_vdown">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.005" />
		<inertia ixx="2.08333333333e-06" ixy="0.0" ixz="0.0" iyy="1.04333333333e-06" iyz="0.0" izz="1.04333333333e-06"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 -0.05 -0.05"/>
			<geometry>
