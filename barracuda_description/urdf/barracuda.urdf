			</geometry>
			<material name="blue"/>
		</visual>
	</link>

	<gazebo reference="frontfin_vdown">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
		<mu2>10.0</mu2>
		<material>Gazebo/Blue</material>
	</gazebo>

	<joint name="frontfin_vdown_joint" type="fixed">
