	<link name="frontfin_vright">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.003" />
			<inertia ixx="2.26e-07" ixy="0.0" ixz="0.0" iyy="2.26e-07" iyz="0.0" izz="4.5e-07"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<box size="0.03 0.03 0.002"/> x-length,y-legth and z-length
			</geometry>
			<material name="blue"/>
		</visual>
	</link>

	<gazebo reference="frontfin_vright">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
		<mu2>10.0</mu2>
		<material>Gazebo/Blue</material>
	</gazebo>

	<joint name="frontfin_vright_joint" type="fixed">
		<parent link="second_link"/>
		<child link="frontfin_vright"/>
		<origin xyz="-0.05 -0.05 0" rpy="0 0 0"/>
	</joint>

	<link name="frontfin_vleft">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.003" />
			<inertia ixx="2.26e-07" ixy="0.0" ixz="0.0" iyy="2.26e-07" iyz="0.0" izz="4.5e-07"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<box size="0.03 0.03 0.002"/> x-length,y-legth and z-length
			</geometry>
			<material name="blue"/>
		</visual>
	</link>

	<gazebo reference="frontfin_vleft">
		<kp>100000.0</kp>
		<kd>100000.0</kd>
		<mu1>10.0</mu1>
		<mu2>10.0</mu2>
		<material>Gazebo/Blue</material>
	</gazebo>

	<joint name="frontfin_vleft_joint" type="fixed">
		<parent link="second_link"/>
		<child link="frontfin_vleft"/>
		<origin xyz="0.05 -0.05 0" rpy="0 0 0"/>
	</joint>

	<link name="camera_link"> camera
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.001" />
			<inertia ixx="9.1875e-09" ixy="0.0" ixz="0.0" iyy="9.1875e-09" iyz="0.0" izz="1.378125e-08"/>
		</inertial>
		<visual>
			<origin rpy="0.0 0 0" xyz="0 0 0"/>
			<geometry>
				<box size="0.0005 0.0005 0.0005"/>
			</geometry>
			<material name="red"/>
		</visual>
	</link>

	<gazebo reference="camera_link">
		<material>Gazebo/Red</material>
	</gazebo>

	<joint name="camera_joint" type="fixed">
		<parent link="ball_link"/>
		<child link="camera_link"/>
		<origin xyz="0 -0.03 0" rpy="0 0 0"/>
	</joint>

<!-- camera -->
	<gazebo reference="camera_link">
		<sensor type="camera" name="camera1">
			<update_rate>15.0</update_rate>
			<camera name="head">

					<pose>0 0 0 0 0 0</pose>

				<horizontal_fov>1.3962634</horizontal_fov>
				<image>
					<width>400</width>
					<height>400</height>
					<format>R8G8B8</format>
				</image>
				<clip>
					<near>0.01</near>
					<far>100</far>
				</clip>
				<noise>
					<type>gaussian</type>
					<stddev>0.007</stddev>
				</noise>
			</camera>
			<plugin name="camera_controller" filename="libgazebo_ros_camera.so">
				<alwaysOn>true</alwaysOn>
				<updateRate>0.0</updateRate>
				<cameraName>barracuda/camera1</cameraName>
				<imageTopicName>image_raw</imageTopicName>
				<cameraInfoTopicName>camera_info</cameraInfoTopicName>
				<frameName>camera_link</frameName>
				<hackBaseline>0.07</hackBaseline>
				<distortionK1>0.0</distortionK1>
				<distortionK2>0.0</distortionK2>
				<distortionK3>0.0</distortionK3>
				<distortionT1>0.0</distortionT1>
				<distortionT2>0.0</distortionT2>
			</plugin>
		</sensor>
	</gazebo>
</robot>
